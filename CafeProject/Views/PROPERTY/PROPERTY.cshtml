@model IEnumerable<CafeProject.Models.property_table>

@{
    ViewBag.Title = "PROPERTY";
    Layout = "~/Views/_LayoutPageMenu.cshtml";
}

<h2>PROPERTY</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Key)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Value)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Key)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Value)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new {  id=item.ProperyID }) |
          
            @Html.ActionLink("Delete", "Delete", new {  id=item.ProperyID }) |
 
        </td>
    </tr>
}

</table>
